    url('../fonts/hunger-icon.ttf?lce5tr') format('truetype'),
    url('../fonts/hunger-icon.woff?lce5tr') format('woff'),
    url('../fonts/hunger-icon.svg?lce5tr#hunger-icon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
